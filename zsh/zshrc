
export PATH="/usr/local/bin:$PATH" # use brew git

alias soa='source ~/.oh-my-zsh/custom/aliases.zsh'

alias gco='git checkout'
alias gcob='git checkout -b'
alias gg='git log'
alias gl='git pull'
alias gai='git add . -p'
alias ga='git add'
alias gaa='git add --all'
alias gcm='git commit -m'
alias gc='git commit'
alias gh='git push'
alias gs='git status'
alias gd='git diff'
alias gsh='git stash'
alias gshl='git stash list'
alias gshp='git stash pop'

alias tm='tmux'
alias tmn='tmux new -s'
alias tma='tmux a -t'

alias filediff=gitFileDiff
gitFileDiff() {
  git diff-tree --no-commit-id --name-only -r HEAD..$1
}

#source ~/bin/git-completion.bash # run if not working

alias zygrib='open /Applications/zyGrib_mac-8.0.1/zyGrib.app/Contents/MacOS/zyGrib'
fpath=(~/.zsh $fpath)

